{
  "name": "nodejs-rag-module",
  "version": "1.0.1",
  "description": "A Node.js TypeScript module for RAG functionality using nomic-embed-text-v1.5 and sqlite-vec",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "ts-node src/index.ts",
    "prepublishOnly": "npm run build && npm test",
    "prepack": "npm run build"
  },
  "keywords": [
    "rag",
    "embedding",
    "vector-database",
    "sqlite",
    "mysql",
    "postgresql",
    "typescript",
    "semantic-search",
    "ai",
    "machine-learning",
    "nlp",
    "retrieval-augmented-generation"
  ],
  "author": "Paul Kolade <olaoyepaulkolade@gmail.com> (https://github.com/Codre01)",
  "homepage": "https://github.com/Codre01/nodejs-rag-module#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Codre01/nodejs-rag-module.git"
  },
  "bugs": {
    "url": "https://github.com/Codre01/nodejs-rag-module/issues"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "license": "MIT",
  "dependencies": {
    "mysql2": "^3.14.2",
    "node-llama-cpp": "^3.10.0",
    "pg": "^8.11.3",
    "sqlite-vec": "^0.1.7-alpha.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.15.5",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
